def my_function(s):
    for i in range(len(s)):
        for j in range(i+1,len(s)):
            if s[i]+s[j]==target:
                return i,j
    return -1

s=[2,7,55,123,14,17]
target=int(input("enter the target value:"))
result=my_function(s)
if result==-1:
    print("not found")
else:
    print("found =",result)
